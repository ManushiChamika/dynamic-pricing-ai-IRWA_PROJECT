+function buildInfo(m: Message) {
+  const info: string[] = []
+  if (m.created_at) info.push(`time: ${new Date(m.created_at).toLocaleString()}`)
+  const modelComputed = m.model || (m.metadata && (m.metadata as any).provider ? `${(m.metadata as any).provider}${m.model ? `:${m.model}` : ''}` : '')
+  if (modelComputed) info.push(`model: ${modelComputed}`)
+  if (m.token_in != null || m.token_out != null) info.push(`tokens: ${m.token_in || 0}/${m.token_out || 0}`)
+  if (m.cost_usd != null) info.push(`cost: $${m.cost_usd}`)
+  if (m.api_calls != null) info.push(`api calls: ${m.api_calls}`)
+  const agentNames = m.agents?.activated || []
+  const agentCount = m.agents?.count ?? agentNames.length
+  if (agentCount > 0) info.push(`agents: ${agentCount}${agentNames.length ? ` (${agentNames.join(', ')})` : ''}`)
+  const toolNames = m.tools?.used || []
+  const toolCount = m.tools?.count ?? toolNames.length
+  if (toolCount > 0) info.push(`tools: ${toolCount}${toolNames.length ? ` (${toolNames.join(', ')})` : ''}`)
+  return info.join('\n')

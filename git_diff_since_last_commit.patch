+    set({ token: t })
+  },
+  login: async (email, password) => {
+    const res = await api('/api/login', { method: 'POST', json: { email, password } })
+    if (res.ok && res.data?.token) {
+      get().setToken(res.data.token)
+      set({ user: res.data.user || null })
+      return { ok: true }

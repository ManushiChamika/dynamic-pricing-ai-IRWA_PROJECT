+        api_calls: x.api_calls ?? null,
+        parent_id: x.parent_id ?? null,
+        metadata: x.metadata ?? null
+      }))
+    }
+    const res = await api('/api/threads/import', { method: 'POST', json: payload })
+    if (res.ok && res.data?.id) return res.data.id
+    return null
+  }
